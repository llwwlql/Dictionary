package cn.edu.ex.ch9.test;

class students{
	private String name;
	protected int age;
	public char sex;
	
	public students(String name,int age,char sex) {
		// TODO Auto-generated constructor stub
		this.name = name;
		this.age = age;
		this.sex = sex;
	}
	public students() {
		// TODO 自动生成的构造函数存根
	}
	
	public void study()
	{
		System.out.println("i need study");
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "姓名 "+this.name+"年龄 "+this.age+"性别   "+this.sex;
	}
}