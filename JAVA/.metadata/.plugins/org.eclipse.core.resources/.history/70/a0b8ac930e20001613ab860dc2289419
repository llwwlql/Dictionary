package reflex;

public class MethodReflex {
	Class cls = students.class;
	Constructor cons = cls.getDeclaredConstructor(new Class[]{String.class,int.class,char.class});
	
	cons.setAccessible(true);
	Object obj = cons.newInstance(new Object[]{"ÀîËÄ",19,'f'});
	System.out.println(obj);
	Method method = cls.getMethod("study", null);
	method.invoke(obj, null);

}
